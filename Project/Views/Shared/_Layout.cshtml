<!doctype html>

<html lang="en"
      class="layout-navbar-fixed layout-menu-fixed layout-compact"
      dir="ltr"
      data-skin="default"
      data-bs-theme="light"
      data-assets-path="/MaterializeUI/assets/"
      data-template="vertical-menu-template">

<partial name="~/Views/Shared/_Head.cshtml" />


<body>
    <!-- Layout wrapper -->
    <div class="layout-wrapper layout-content-navbar">
        
        <div class="layout-container">
            <!-- Menu -->

            <partial name="~/Views/Shared/_Sidebar.cshtml" />
           
            <!-- / Menu -->
            <!-- Layout container -->
            <div class="layout-page">
                <!-- Navbar -->
                
                
                <partial name="~/Views/Shared/_Header.cshtml" />
                <!-- / Navbar -->
                

                <!-- Content wrapper -->
                <div class="content-wrapper">

                    @RenderBody()

                    <!--Footer-->
                    <partial name="~/Views/Shared/_Footer.cshtml" />
                    <!--Footer-->

                    <div class="content-backdrop fade"></div>
                </div>
                <!-- Content wrapper -->
            </div>
            <!-- / Layout page -->
        </div>

        <!-- Overlay -->
        <div class="layout-overlay layout-menu-toggle"></div>

        <!-- Drag Target Area To SlideIn Menu On Small Screens -->
        <div class="drag-target"></div>
    </div>
    <!-- / Layout wrapper -->
    <!-- Core JS -->
    <!-- build:js assets/vendor/js/theme.js  -->

    <partial name="~/Views/Shared/_Script.cshtml" />

    <!-- Page JS -->
</body>
</html>
